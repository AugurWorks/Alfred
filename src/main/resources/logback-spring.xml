<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE logback>
<configuration>

    <include resource="logback-appenders.xml" />

    <logger name="alfred" level="INFO">
        <appender-ref ref="FLUENT" />
    </logger>

    <root>
        <level value="INFO" />
        <appender-ref ref="STDOUT" />
        <appender-ref ref="FLUENT" />
    </root>

</configuration>